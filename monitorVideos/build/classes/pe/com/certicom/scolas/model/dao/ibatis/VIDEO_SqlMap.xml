    <select id="listarVideos" resultMap="ibatorgenerated_BaseResultMap">
    select v.id_video, v.des_video, v.des_ruta_video, v.ind_activo
    from
        CONFIGURACION_EMISOR_LLAMADOS c,
        LISTA_VIDEOS_VIDEO lvv,
        LISTA_VIDEOS lv,
        VIDEO v
    where c.id_lista_videos = lv.id_lista_videos AND
          lv.id_lista_videos = lvv.id_lista_videos AND
          lvv.id_video = v.id_video AND
          c.id_configuracion_emisor_llamados = #idConfiguracion# AND
          (v.ind_vigencia_indefinida='1' OR GETDATE() between v.fec_inivig and v.fec_finvig)
    order by
          lvv.num_orden ASC
  </select>

